<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="451.0"
            prefWidth="623.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="TennisBallGames.AddMatchController">
    <top>
        <Label text="Add New Match Form" BorderPane.alignment="CENTER">
            <BorderPane.margin>
                <Insets top="10.0"/>
            </BorderPane.margin>
            <font>
                <Font name="System Bold" size="12.0"/>
            </font>
        </Label>
    </top>
    <left>
        <AnchorPane prefHeight="372.0" prefWidth="187.0" BorderPane.alignment="CENTER">
            <children>
                <Label layoutX="80.0" layoutY="129.0" text="Home Team"/>
                <Label layoutX="80.0" layoutY="177.0" text="Away Team"/>
            </children>
        </AnchorPane>
    </left>
    <center>
        <AnchorPane prefHeight="338.0" prefWidth="327.0" BorderPane.alignment="CENTER">
            <children>
                <ComboBox fx:id="homeTeamBox" layoutX="14.0" layoutY="123.0" prefWidth="150.0"/>
                <ComboBox fx:id="visitorTeamBox" layoutX="14.0" layoutY="174.0" prefWidth="150.0"/>
                <Label layoutX="14.0" layoutY="156.0" text="VS."/>
            </children>
        </AnchorPane>
    </center>
    <bottom>
        <AnchorPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
            <children>
                <Button fx:id="cancelButton" layoutX="420.0" layoutY="87.0" mnemonicParsing="false" onAction="#cancel"
                        text="Cancel"/>
                <Button fx:id="saveButton" layoutX="519.0" layoutY="87.0" mnemonicParsing="false" onAction="#save"
                        text="Save"/>
            </children>
        </AnchorPane>
    </bottom>
</BorderPane>
